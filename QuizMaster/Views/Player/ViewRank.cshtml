@model IEnumerable<Player>
@{
    ViewData["Title"] = "View Rank";
}
<h2>Top Players</h2>
<table class="table table-dark mt-4">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Username</th>
            <th scope="col">Score</th>
        </tr>
    </thead>
    <tbody>
        @{int i = 1;
            foreach (var player in Model)
            {
                <tr>
                    <th scope="row">@i</th>                
                    <td>@TempData["Name"]</td>                                
                    <td>@player.Score</td>
                </tr>
                i++;
            }
        }
    </tbody>
</table>